	<query language="sql"><![CDATA[SELECT 
    u.idUsuario,
    u.nombreUsuario,
    u.dni,
    u.RUC,
    u.razonsocial,
    COUNT(ov.idOrdenVenta) AS total_ordenes
FROM 
    Usuario u
JOIN 
    OrdenVenta ov ON u.idUsuario = ov.idUsuario
WHERE 
    ov.estado_compra != 'pendiente'
    AND ov.activo = 1
    AND MONTH(ov.fecha_orden) = MONTH(CURDATE())
	AND YEAR(ov.fecha_orden) = YEAR(CURDATE())
GROUP BY 
    u.idUsuario, u.nombreUsuario, u.dni, u.RUC, u.razonsocial
ORDER BY 
    total_ordenes DESC
 LIMIT 10

]]></query>
